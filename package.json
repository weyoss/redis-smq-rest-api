{
  "name": "redis-smq-rest-api",
  "version": "1.0.0-rc.1",
  "description": "A RESTful API for RedisSMQ",
  "author": "Weyoss <weyoss@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "redis-smq",
    "rest api",
    "priority queue",
    "scheduler",
    "broker",
    "message broker",
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq-rest-api.git"
  },
  "homepage": "https://github.com/weyoss/redis-smq-rest-api",
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq-rest-api/issues"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@koa/bodyparser": "5.1.0",
    "@koa/cors": "5.0.0",
    "@koa/router": "12.0.1",
    "@types/bunyan": "1.8.11",
    "ajv": "8.13.0",
    "awilix": "9.0.0",
    "bluebird": "3.7.2",
    "bunyan": "2.0.5",
    "koa": "2.14.2",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "koa2-swagger-ui": "5.10.0",
    "lodash": "4.17.21",
    "temp-dir": "3.0.0",
    "typescript-json-schema": "0.63.0",
    "uuid": "9.0.1"
  },
  "peerDependencies": {
    "redis-smq": "8.0.0-rc.24"
  },
  "devDependencies": {
    "@openapi-contrib/json-schema-to-openapi-schema": "3.0.1",
    "@types/bluebird": "3.5.42",
    "@types/koa__cors": "5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/koa-mount": "4.0.5",
    "@types/koa-static": "4.0.4",
    "@types/lodash": "4.14.202",
    "@types/node": "18.19.1",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitest/coverage-v8": "1.5.3",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "12.5.0",
    "openapi-types": "12.1.3",
    "prettier": "3.0.3",
    "supertest": "7.0.0",
    "type-coverage": "2.27.0",
    "typescript": "4.9.4",
    "vitest": "1.5.3"
  },
  "scripts": {
    "build": "scripts/build.sh",
    "test": "vitest",
    "release": "scripts/release/release.sh",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "type-coverage": "type-coverage --strict --at-least 100  --ignore-files \"dist/**/*\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.(ts)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.(json|js)": [
      "prettier --write",
      "git add"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18"
  }
}
